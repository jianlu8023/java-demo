<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.jianlu8023.example.integration.web.database.mysql.mapper.TbUserMapper">

    <resultMap id="BaseResultMap" type="com.github.jianlu8023.example.integration.web.database.mysql.entity.TbUser">
